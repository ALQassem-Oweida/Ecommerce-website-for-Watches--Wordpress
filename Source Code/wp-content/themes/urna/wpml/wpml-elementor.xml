<shortcodes>
    <elementor-widgets>
        <widget name="tbay-video">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="Video URL" editor_type="LINE">video_url</field>
            </fields>
        </widget>
        <widget name="tbay-custom-menu">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
            </fields>
        </widget>
        <widget name="tbay-heading">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Description" editor_type="AREA">heading_description</field>
            </fields>
        </widget>
        <widget name="tbay-features">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
            <fields-in-item items_of="features">
                <field type="Features: Title" editor_type="LINE">feature_title</field>
                <field type="Features: Description" editor_type="AREA">feature_desc</field>
            </fields-in-item>
        </widget>
        <widget name="tbay-brands">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
            <fields-in-item items_of="brands">
                <field type="Link to" editor_type="LINK" key_of="link">brand_link</field>
            </fields-in-item>
        </widget>
        <widget name="tbay-posts-grid">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
        </widget>
        <widget name="tbay-our-team">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
            <fields-in-item items_of="our_team">
                <field type="Our Team: Name" editor_type="LINE">our_team_name</field>
                <field type="Our Team: Job" editor_type="LINE">our_team_job</field>
                <field type="Our Team: FaceBook Link" editor_type="LINK" key_of="link">our_team_link_fb</field>
                <field type="Our Team: Twitter Link" editor_type="LINK" key_of="link">our_team_link_tw</field>
                <field type="Our Team: Goole Plus Link" editor_type="LINK" key_of="link">our_team_link_gg</field>
                <field type="Our Team: Linkin Link" editor_type="LINK" key_of="link">our_team_link_linkin</field>
                <field type="Our Team: Instagram Link" editor_type="LINK" key_of="link">our_team_link_instaram</field>
            </fields-in-item>
        </widget>
        <widget name="tbay-testimonials">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
            <fields-in-item items_of="testimonials">
                <field type="Testimonials Items: Name" editor_type="LINE">testimonial_name</field>
                <field type="Testimonials Items: Job" editor_type="LINE">our_team_job</field>
                <field type="Testimonials Items: Excerpt" editor_type="AREA">testimonial_excerpt</field>
            </fields-in-item>
        </widget>
        <widget name="tbay-social-icons">
            <fields>
                <field type="Recently Viewed: Title" editor_type="LINE">header_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
            <fields-in-item items_of="social_icon_list">
                <field type="Social Icons: Link" editor_type="LINK" key_of="link">link</field>
            </fields-in-item>
        </widget>
        <widget name="tbay-newsletter">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
        </widget>
        <widget name="tbay-search-form">
            <fields>
                <field type="Placeholder Text" editor_type="LINE">placeholder_text</field>
                <field type="Text Validate Input Search" editor_type="LINE">vali_input_search</field>
                <field type="Button Search Text" editor_type="LINE">text_button_search</field>
                <field type="Search in Categories Text" editor_type="LINE">text_categories_search</field>
            </fields>
        </widget>
        <widget name="tbay-mini-cart">
            <fields>
                <field type="Mini-Cart Title" editor_type="LINE">title_mini_cart</field>
            </fields>
        </widget>
        <widget name="tbay-header-flash-sales">
            <fields>
                <field type="Text" editor_type="LINE">header_text</field>
            </fields>
        </widget>
        <widget name="tbay-header-recently-viewed">
            <fields>
                <field type="Recently Viewed: Title" editor_type="LINE">header_title</field>
                <field type="Recently Viewed: Empty Result - Custom Paragraph" editor_type="AREA">empty</field>
            </fields>
        </widget>
        <widget name="tbay-wishlist">
            <fields>
                <field type="Title" editor_type="LINE">title_wishlist</field>
            </fields>
        </widget>
        <widget name="tbay-compare">
            <fields>
                <field type="Title" editor_type="LINE">title_compare</field>
            </fields>
        </widget>
        <widget name="tbay-products">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="Show More: Text Button" editor_type="LINE">text_button</field>
            </fields>
        </widget>
        <widget name="tbay-product-category">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
        </widget>
        <widget name="tbay-product-tabs">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
            <fields-in-item items_of="list_product_tabs">
                <field type="Tab Item: Title" editor_type="LINE">product_tabs_title</field>
            </fields-in-item>
        </widget>
        <widget name="tbay-product-categories-tabs">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="Show More: Text Button" editor_type="LINE">text_button</field>
            </fields>
        </widget>
        <widget name="tbay-list-categories-product">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="Show More: Text Button" editor_type="LINE">text_button</field>
            </fields>
        </widget>
        <widget name="tbay-custom-image-list-categories">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="Show More: Text Button" editor_type="LINE">text_button</field>
            </fields>
        </widget>
        <widget name="tbay-product-count-down">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="Title Date" editor_type="LINE">countdown_title</field>
            </fields>
        </widget>
        <widget name="tbay-custom-image-list-tags">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="Show More: Text Button" editor_type="LINE">text_button</field>
            </fields>
        </widget>
        <widget name="tbay-product-recently-viewed">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="Empty Result - Custom Paragraph" editor_type="AREA">empty</field>
            </fields>
        </widget>
        <widget name="tbay-product-flash-sales">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="Title Date" editor_type="LINE">date_title</field>
                <field type="Title deal ended" editor_type="LINE">date_title_ended</field>
                <field type="Custom Text Link" editor_type="LINE">readmore_text</field>
                <field type="Custom Link" editor_type="LINK" key_of="link">readmore_link</field>
            </fields>
        </widget>
        <widget name="tbay-categories-tabs-with-banner">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
            <fields-in-item items_of="categories">
                <field type="Categories: External link" editor_type="LINE">link</field>
            </fields-in-item>
        </widget>
        <widget name="tbay-top-notification">
            <fields-in-item items_of="notifications">
                <field type="Item: Description" editor_type="VISUAL">description</field>
            </fields-in-item>
        </widget>
        <widget name="tbay-products-banner-menu">
            <fields>
                <field type="Title" editor_type="LINE">heading_title</field>
                <field type="Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="Custom Link" editor_type="LINK" key_of="link">banner_link</field>
            </fields>
        </widget>
    </elementor-widgets>
</wpml-config>